{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "astro": "^5.16.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "sass": "^1.77.8",
    "typescript-eslint": "^8.52.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "trailingComma": "es5",
    "overrides": [
      {
        "files": "**/*.ts",
        "options": {
          "parser": "typescript"
        }
      },
      {
        "files": [
          "**/*.ts",
          "**/*.tsx",
          "**/*.js",
          "**/*.jsx",
          "**/*.astro",
          "**/*.mjs",
          "**/*.cjs"
        ],
        "options": {
          "tabWidth": 4
        }
      }
    ]
  }
}
