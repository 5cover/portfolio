{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/preact": "^4.1.3",
    "@astrojs/rss": "^4.0.15",
    "@astrojs/sitemap": "^3.7.0",
    "astro": "^5.16.15",
    "preact": "^10.28.2",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "typescript-eslint": "^8.53.1"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "trailingComma": "es5",
    "overrides": [
      {
        "files": "**/*.ts",
        "options": {
          "parser": "typescript"
        }
      },
      {
        "files": [
          "**/*.ts",
          "**/*.tsx",
          "**/*.js",
          "**/*.jsx",
          "**/*.astro",
          "**/*.mjs",
          "**/*.cjs"
        ],
        "options": {
          "tabWidth": 4
        }
      }
    ]
  }
}
