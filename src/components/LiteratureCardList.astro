---
import type { Item, LocalizedItem } from '../content/config';
import type { Entry } from '../lib/content';
import LiteratureCard from './LiteratureCard.astro';
interface Props {
    langLabels: {
        fmtTitle: string;
    };
    entries: readonly Entry<LocalizedItem<'literature'>>[];
    headingLevel?: number;
}

const { langLabels, entries, headingLevel = 3 } = Astro.props;
---

<ul class='lvl list-detail-split-card'>
    {entries.map(entry => <LiteratureCard langLabels={langLabels} entry={entry} headingLevel={headingLevel} />)}
</ul>
