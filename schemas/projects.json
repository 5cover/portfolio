{
    "type": "object",
    "title": "Projects",
    "$schema": "https://json-schema.org/draft-07/schema",
    "additionalProperties": {
        "type": "object",
        "allOf": [
            {
                "$ref": "util/detail.json"
            },
            {
                "required": [
                    "context",
                    "start-date",
                    "tags",
                    "team",
                    "technologies"
                ],
                "properties": {
                    "end-date": {
                        "format": "date",
                        "type": "string"
                    },
                    "start-date": {
                        "format": "date",
                        "type": "string"
                    },
                    "tags": {
                        "uniqueItems": true,
                        "type": "array",
                        "items": {
                            "$ref": "tags.json#/propertyNames"
                        }
                    },
                    "team": {
                        "uniqueItems": true,
                        "type": "array",
                        "items": {
                            "$ref": "definitions.json#/propertyNames"
                        }
                    },
                    "technologies": {
                        "uniqueItems": true,
                        "type": "array",
                        "items": {
                            "$ref": "definitions.json#/propertyNames"
                        }
                    }
                }
            }
        ]
    }
}