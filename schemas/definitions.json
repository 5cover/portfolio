      "type",
      "wiki",
      "synopsis"
    ],
    "properties": {
      "name": {
        "type": "object",
        "required": [
          "full"
        ],
        "properties": {
          "full": {
            "title": "Full name",
            "$ref": "util/html.json"
          },
          "abbr": {
            "title": "Abbreviated name",
            "$ref": "util/html.json"
          },
          "short": {
            "title": "Short name",
            "$ref": "util/html.json"
          }
        }
      },
      "synopsis": {
        "title": "Synopsis. Should not restate name",
        "$ref": "util/html.json"
      },
      "type": {
        "$ref": "types.json#/propertyNames"
      },
      "wiki": {
        "type": "string",
        "minLength": 1
      },
      "background": {
        "type": "string",
        "minLength": 1
      },
      "logo": {
        "$ref": "util/graphic.json"
      }
    }
